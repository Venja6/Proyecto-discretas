k=0
ver si el original es conexo
k=1
for(i=0; i<vertices)
	eliminacion()
	conexidad()
	si se rompe k=1
k++
for(i=0; i<vertices)
	for(i+1; j<vertices)
		eliminacion()
		conexidad()
		si se rompe K=2
k++
for(i=0; i<vertices, ++)
	for(j=i+1; j<vertices, ++)
		for(k=j+1; k<vertices, ++)
		eliminacion()
		conexidad()
		si se rompe K=3
k++
for(i=0; i<vertices)
	for(j=i+1; j<vertices)
		for(k=j+1; k<vertices)
			for(l=k+1, l<vertices)
			eliminacion()
			conexidad()
			si se rompe K=4


